{
  "name": "astray",
  "version": "1.0.0",
  "description": "Quercus Insolita template project.",
  "scripts": {
    "start:client": "node src/client/scripts/start.js",
    "build:client": "node src/client/scripts/build.js",
    "build:server": "tsc -p src/server",
    "clean:build": "rm -rf dist",
    "clean": "rm -rf node_modules dist",
    "build": "npm run clean:build && npm run build:client && npm run build:server",
    "start": "npm run build && node dist/index.js"
  },
  "repository": {
    "type": "git",
