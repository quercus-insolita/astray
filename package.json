  ],
  "author": "Quercus Insolita team <paavlenko.andrew@gmail.com> (Andrew Pavlenko <paavlenko.andrew@gmail.com>, Olha Yakymovych<olha.yakymovych@gmail.com>, Danylo Petrakivskyi<danylopetrik@gmail.com>)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/quercus-insolita/astray/issues"
